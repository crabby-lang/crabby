// Crabby's Example network usage - EXPERIMENTAL

import http from "http"

def network_example() {
    let server = http.listen("127.0.0.1", 8080)
    server.send("Hello!")
    let response = server.receive()

    let client = http.connect("127.0.0.1", 8080)
    client.send("Hello server!")
}

def main() {
    network_example()
}

