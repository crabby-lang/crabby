{
  "name": "crabby-lang",
  "displayName": "Crabby Programming Language",
  "description": "Crabby language support for Vscode",
  "version": "0.4.19",
  "publisher": "ingStudiosOfficial",
  "repository": {
    "type": "git",
    "url": "https://github.com/crabby-lang/crabby"
  },
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "icon": "icons/crabby.png",
  "contributes": {
    "languages": [
      {
        "id": "crabby",
        "aliases": [
          "Crabby",
          "crabby"
        ],
        "extensions": [
          ".crab",
          ".cb"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "crabby",
        "scopeName": "source.crabby",
        "path": "./syntaxes/crabby.tmLanguage.json"
      }
    ],
    "iconThemes": [
      {
        "id": "crabby-icons",
        "label": "Crabby Icons",
        "path": "./icons/crabby-icon.json"
      }
    ]
  }
}
